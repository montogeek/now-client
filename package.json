{
  "name": "now-api",
  "version": "0.3.0",
  "description": "Node.js wrapper for the 𝚫 now API",
  "main": "./dist/now.js",
  "scripts": {
    "prepublish": "./node_modules/.bin/babel -d ./dist src && npm run gen-doc",
    "eslint": "./node_modules/.bin/eslint ./src",
    "gen-doc": "./node_modules/.bin/jsdoc2md --src src/now.js -t README.hbs > README.md",
    "test": "./node_modules/.bin/mocha test/index.js --compilers js:babel-register"
  },
  "files": [
    "dist"
  ],
  "repository": "timolins/now-api",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-console": 0
    },
    "plugins": [
      "mocha"
    ],
    "env": {
      "mocha": true
    }
  },
  "keywords": [
    "now",
    "api",
    "zeit",
    "deploy"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Timo Lins <me@timo.sh> (https://timo.sh/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.13.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "jsdoc-to-markdown": "^1.3.6",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "axios": "^0.12.0"
  }
}
